{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Server",
        "type": "dart",
        "request": "launch",
        "program": "bin/server.dart",
        "cwd": "${workspaceFolder}/server"
      },
      {
        "name": "Run Flutter App",
        "type": "dart",
        "request": "launch",
        "program": "lib/main.dart",
        "flutterMode": "debug",
        "deviceId": "chrome",
        "cwd": "${workspaceFolder}/client"
      }
    ],
    "compounds": [
      {
        "name": "Run Server and Flutter App",
        "configurations": ["Run Server", "Run Flutter App"]
      }
    ]
  }
  